{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "CBC-meta-data",
  "description": "The meta-data for a single CBC trigger",
  "type": "object",
  "properties": {
    "publications": {
      "description": "The publications meta-data",
      "type": "object",
      "properties": {
        "papers": {
          "description": "The papers this trigger appears in",
          "type": "array",
          "items": {
            "type": "string"
           },
        },
      },
    },
    "parameter_estimation": {
      "description": "The parameter estimation meta-data",
      "type": "object",
      "properties": {
        "analysts": {
          "description": "The names of all analysts involved",
          "type": "array"
          "items": {
            "type": "string"
           },
        }

      }
    }
  },
}
